								"exec": [
									"// Test to check if the response status code is 200\r",
									"pm.test(\"Response status code is 200\", function () {\r",
									"    pm.expect(pm.response.code).to.equal(200);\r",
									"});\r",
									"\r",
									"// Test to check if the response time is less than 200ms\r",
									"pm.test(\"Response time is less than 300ms\", function () {\r",
									"    pm.expect(pm.response.responseTime).to.be.below(1000);\r",
									"});\r",
									"\r",
									"// Test to check if the response has the required fields\r",
									"pm.test(\"Response has the required fields\", function () {\r",
									"    const responseData = pm.response.json();\r",
									"    \r",
									"    pm.expect(responseData).to.be.an('object');\r",
									"    pm.expect(responseData).to.include.all.keys('bookingid', 'booking');\r",
									"    \r",
									"    const booking = responseData.booking;\r",
									"    pm.expect(booking).to.include.all.keys('firstname', 'lastname', 'totalprice', 'depositpaid', 'bookingdates', 'additionalneeds');\r",
									"    \r",
									"    const bookingdates = booking.bookingdates;\r",
									"    pm.expect(bookingdates).to.include.all.keys('checkin', 'checkout');\r",
									"});\r",
									"\r",
									"// Test to check if bookingdates contain checkin and checkout fields\r",
									"pm.test(\"Bookingdates contain checkin and checkout fields\", function () {\r",
									"    const responseData = pm.response.json();\r",
									"    \r",
									"    pm.expect(responseData.booking.bookingdates.checkin).to.exist;\r",
									"    pm.expect(responseData.booking.bookingdates.checkout).to.exist;\r",
									"});\r",
									"\r",
									"// Test to check if firstname and lastname are non-empty strings\r",
									"pm.test(\"Firstname and lastname are non-empty strings\", function () {\r",
									"  const responseData = pm.response.json();\r",
									"  \r",
									"  pm.expect(responseData).to.be.an('object');\r",
									"  pm.expect(responseData.booking.firstname).to.be.a('string').and.to.have.lengthOf.at.least(1, \"Firstname should not be empty\");\r",
									"  pm.expect(responseData.booking.lastname).to.be.a('string').and.to.have.lengthOf.at.least(1, \"Lastname should not be empty\");\r",
									"});\r",
